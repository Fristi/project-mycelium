{
  "name": "mycelium-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:android": "tsc && vite build && npx cap sync && npx cap open android",
    "run:android": "tsc && vite build && npx cap sync && npx cap run android",
    "preprov": "earthly ../backend/+dev && docker compose -f ../docker-compose.dev.yml up",
    "stage": "earthly ../backend/+stage && docker compose -f ../docker-compose.dev.yml up",
    "prettier": "npx prettier . --write",
    "prettier-check": "npx prettier . --check"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.0",
    "@capacitor-community/bluetooth-le": "^3.0.0",
    "@capacitor/android": "^5.4.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/browser": "^5.0.6",
    "@capacitor/core": "^5.0.4",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@tanstack/react-query": "^4.32.0",
    "axios": "^0.24.0",
    "formik": "^2.4.2",
    "moment": "^2.29.4",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.11.2",
    "recharts": "^2.7.2",
    "zod": "^3.21.4",
    "zod-formik-adapter": "^1.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.4",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "@types/react-query": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.24",
    "prettier": "3.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
